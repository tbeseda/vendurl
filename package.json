{
  "name": "vendurl",
  "version": "0.2.2",
  "description": "Vendor (verb: download and store) a JS dependency from a URL.",
  "author": "tbeseda",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbeseda/vendurl.git"
  },
  "type": "module",
  "main": "index.js",
  "bin": {
    "vendurl": "index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint.fix": "npm run lint -- --fix",
    "release": "release-it",
    "test": "cd test && bash ./test.sh"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "release-it": "^17.0.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "ignorePatterns": [
      "**/vendor"
    ],
    "rules": {
      "space-unary-ops": 0,
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  },
  "keywords": [
    "javascript",
    "vendor",
    "bundle",
    "esm.sh"
  ]
}

{
  "type": "module",
  "scripts": {
    "postinstall": "vendurl --clean -y -v"
  },
  "devDependencies": {
    "vendurl": "file:../.."
  },
  "vendurl": {
    "_destination": "./vendor",
    "_bundle": true,
    "_provider": "https://esm.sh",
    "packages": {
      "temporal.js": "@js-temporal/polyfill",
      "chalk4.mjs": "chalk@4",
      "leftpad.cjs": "https://unpkg.com/leftpad@0.0.1/index.js",
      "yaml.js": {
        "package": "yaml",
        "version": "1.10.0",
        "provider": "https://unpkg.com",
        "destination": "./vendor/yaml",
        "bundle": false,
        "target": "es2021"
      },
      "invalid": [
        "arrays",
        "are",
        "not",
        "supported"
      ],
      "FAKE": "doesntexist@6.6.6"
    }
  }
}
